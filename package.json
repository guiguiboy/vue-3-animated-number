    "vite": "^4.1.0",
    "vitepress": "^1.0.0-alpha.65",
    "vue-tsc": "^1.0.24"
  },
  "engines": {
    "node": ">=18.15.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/guiguiboy/vue-3-animated-number.git"
